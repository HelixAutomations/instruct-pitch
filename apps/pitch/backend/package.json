{
  "name": "helix-backend",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
  "start": "node server.js",
  "build": "tsc",
  "test": "node disabledMode.test.js && node confirmPayment.test.js && node email.test.js && node logRedaction.test.js",
  "dev:all": "node scripts/dev-all.js",
  "dev:client": "npm run build --prefix ../client",
  "dev:mock": "node scripts/mock_local_server.js",
  "dev:decoupled": "npm --prefix ../../../decoupled-functions run start",
  "dev:local": "npm run dev:client && concurrently -k \"npm:dev:mock\" \"npm:dev:decoupled\""
  ,
  "dev:hot": "concurrently -k \"npm --prefix ../client run dev\" \"node scripts/mock_local_server.js\" \"npm --prefix ../../../decoupled-functions run start\""
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@azure/identity": "^3.3.0",
    "@azure/keyvault-secrets": "^4.9.0",
    "@azure/storage-blob": "^12.14.0",
    "axios": "^1.6.7",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "mssql": "^11.0.1",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.1.5",
    "nodemailer": "^6.9.11"
  },
  "devDependencies": {
    "nock": "^14.0.10",
    "typescript": "^5.0.0",
    "concurrently": "^8.2.2"
  }
}
