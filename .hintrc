{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/aria": [
      "default",
      {
        "aria-valid-attr-value": "off",
        "aria-allowed-attr": "off",
        "aria-required-attr": "off"
      }
    ],
    "no-inline-styles": "off",
    "typescript-config/is-valid": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "image-rendering: crisp-edges"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ]
  }
}